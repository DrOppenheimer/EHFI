#job test_analysis.1
-f 16.Qiime.100p.included.norm.qiime_table -g AMETHST.groups -m euclidean -t dataset_rand -z qiime_pipe -q qiime_table -o test_a -cleanup
-f 16.Qiime.100p.included.norm.qiime_table -g AMETHST.groups -m euclidean -t rowwise_rand -z qiime_pipe -q qiime_table -o test_b -cleanup
-m pattern -w test_a -b test_b -o test_analysis.between_within.P_VALUE_SUMMARY

#job test_analysis.2
-f 16.Qiime.100p.included.norm.qiime_table -g AMETHST.groups -m bray-curtis -t dataset_rand -z qiime_pipe -q qiime_table -o test_a -cleanup
-f 16.Qiime.100p.included.norm.qiime_table -g AMETHST.groups -m bray-curtis -t rowwise_rand -z qiime_pipe -q qiime_table -o test_b -cleanup
-m pattern -w test_a -b test_b -o test_analysis.between_within.P_VALUE_SUMMARY
